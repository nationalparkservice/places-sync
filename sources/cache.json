{
  "name": "cache",
  "description": "The database that keeps track of all synchronizations",
  "connection": {
    "type": "sqlite",
    "filePath": "{{env.HOME}}/.sync/cache.sqlite"
  },
  "columns": [{
    "name": "key",
    "type": "text"
  }, {
    "name": "process",
    "type": "text"
  }, {
    "name": "source",
    "type": "text"
  }, {
    "name": "hash",
    "type": "test"
  }, {
    "name": "last_update",
    "type": "numeric"
  }, {
    "name": "data",
    "type": "blob"
  }, {
    "name": "removed",
    "type": "numeric"
  }],
  "tableName": "master",
  "removedField": "removed",
  "lastUpdatedField": "last_update",
  "primaryKey": ["process", "source", "key"]
}
